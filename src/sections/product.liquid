
 
<div data-section-id="{{ section.id }}" data-section-type="product" data-enable-history-state="true" itemscope itemtype="http://schema.org/Product">

  {%- assign current_variant = product.selected_or_first_available_variant -%}
  {%- assign featured_image = current_variant.featured_image | default: product.featured_image -%}

  <meta itemprop="name" content="{{ product.title }}{% unless product.has_only_default_variant %} - {{ current_variant.title }}{% endunless %}">
  <meta itemprop="url" content="{{ shop.url }}{{ current_variant.url }}">
  <meta itemprop="brand" content="{{ product.vendor }}">
  <meta itemprop="image" content="{{ featured_image | img_url: '600x600' }}">
  <meta itemprop="description" content="{{ product.description | strip_html | escape }}">


  <section class="product" data-product>
      <div class="product__inner">

        <div class="product__images">          
           {% if featured_image != blank and product.images.size == 1 %}
              <div>
                <picture>
                  <source media="(min-width: 960px)" data-srcset="{{ featured_image | img_url: '1000x' }}" >
                  <source media="(min-width: 700px)" data-srcset="{{ featured_image | img_url: '800x' }}" >
                  <img src="{{ featured_image | img_url: '600x' }}"  alt="{{ featured_image.alt | escape }}"  data-product-featured-image>
                </picture>
              </div>
            {% endif %}
            
            {% if product.images.size > 1  and featured_image != blank %}
               <div data-product-images  data-product-images-slideshow >
                  <ul>
                   {% for image in product.images  %}
                     <li>
                       <picture>
                           <source media="(min-width: 960px)" data-srcset="{{ image.src | img_url: '1000x' }}" >
                           <source media="(min-width: 700px)" data-srcset="{{ image.src | img_url: '800x' }}" >
                           <img src="{{ image.src | img_url: '600x' }}"  alt="{{ featured_image.alt | escape }}" data-product-image>
                         </picture>
                     </li>
                   {% endfor %}
                 </ul>
               </div>
            {% endif %}
         </div>
    
        <div class="product__information" data-product-info>
                      
              <h1 class="product__information__title">{{ product.title }}</h1>
                      
              <div data-price-wrapper class="product__information__pricing">
                <span data-product-price>
                  {{ current_variant.price | money }}
                </span>
                          
                {% if product.compare_at_price_max > product.price %}
                  <span class="visually-hidden" data-compare-text>{{ 'products.product.regular_price' | t }}</span>
                  <s data-compare-price>
                    {% if current_variant.compare_at_price > current_variant.price %}
                      {{ current_variant.compare_at_price | money }}
                    {% endif %}
                  </s>
                {% endif %}
              </div>
                      
              <div class="row">
                <div class="col-sm-12 col-md-6  col-lg-12">
                  <div class="product__actions" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
                    <meta itemprop="priceCurrency" content="{{ shop.currency }}">
                    <meta itemprop="price" content="{{ current_variant.price | divided_by: 100.00 }}">
                    <link itemprop="availability" href="http://schema.org/{% if current_variant.available %}InStock{% else %}OutOfStock{% endif %}">
                  
                    <form  action="/cart/add" method="post" enctype="multipart/form-data"
                                      id="AddToCartForm--{{ section.id }}">
                              
                    
                              
                      {% unless product.has_only_default_variant %}
                        {% for option in product.options_with_values %}
                          <div class="selector-wrapper js">
                    
                            <!--Needed for radio buttons to work--> 
                            <div class="visually-hidden">
                              <label for="SingleOptionSelector-{{ forloop.index0 }}">
                                {{ option.name }}
                              </label>
                                                
                              <select
                                id="SingleOptionSelector-{{ forloop.index0 }}"
                                data-single-option-selector
                                data-index="option{{ option.position }}">
                                {% for value in option.values %}
                                  <option
                                    value="{{ value | escape }}"
                                    {% if option.selected_value == value %}selected="selected"{% endif %}>
                                      {{ value }}
                                  </option>
                                {% endfor %}
                              </select>
                            </div>
                            <!--END--> 
                             
                    
                            <div class="product__options  flex-grid">
                    
                              <span class="product__options__label">
                               Select {{ option.name }}
                              </span>
                  
                               {%if option.name == 'Size'%}
                              <button type="button"  title="Show Size Guide" class="size-chart-trigger" data-size-chart-trigger>
                                Size Guide
                              </button>
                              {%endif%}
                              
                              {%for value in option.values %}
                                {% assign variant_label_state = true %}
                      
                                {% unless product.variants[forloop.index0].available  %}
                                  {% assign variant_label_state = false %}
                                {% endunless %}
                      
                                <div class="product__options__option  flex-grid-cell {% unless variant_label_state %} is-sold-out {% endunless %}">
                                    <input 
                                      type="radio" 
                                      name="{{option.name}}" 
                                      data-option-selector
                                      data-option-index="option{{option.position}}"
                                      value="{{ value | escape }}"
                                      id="{{option.name | handleize}}-{{value | escape | handleize}}">
                                    <label for="{{option.name | handleize}}-{{value | escape | handleize}}" class="{{option.name | downcase}}  flex-grid  flex-grid--center flex-grid--justify-center">
                                      {{value}}
                                    </label>
                                </div>
                              {%endfor%}

                              {%if option.name == 'Size'%}
                                 <div class="product__model-sizing">{{ product.metafields['model-details']['model-sizing'] }}</div>
                              {%endif%}

                            </div>
                          </div>
                        {% endfor %}
                      {% endunless %}
                               
                      <div class="product__qty">
                    
                        <label for="Quantity">
                        {{ 'products.product.quantity' | t }}
                       </label>
                    
                        <div class="product__qty__actions">
                          <input type="number" id="Quantity" name="quantity" value="1" min="1">
                        </div>
                      </div>     
                    
                      <!--Back for when there is no JS activated--> 
                     <select name="id" class="no-js" data-product-select>
                        {% for variant in product.variants %}
                          <option
                            {% if variant == current_variant %}selected="selected"{% endif %}
                            {% unless variant.available %}disabled="disabled"{% endunless %}
                            value="{{ variant.id }}">
                              {{ variant.title }}
                          </option>
                        {% endfor %}
                      </select>
                    
                      <button
                        type="submit"
                        name="add"
                        class="button  button--medium  button--expanded  button--primary product__buy-btn  is-disabled"
                        data-add-to-cart
                        id="AddToCart--{{section.id}}"
                        {% unless current_variant.available %}disabled="disabled"{% endunless %}>
                          <span data-add-to-cart-text>
                              {{ 'products.product.add_to_cart' | t }}
                          </span>
                      </button>
                                  
                    </form>
                  </div>
                  
                  {% unless product == empty %}
                    <script type="application/json" data-product-json>
                      {{ product | json }}
                    </script>
                  {% endunless %}
                </div>

                <div class="col-sm-12 col-md-6  col-lg-12">                
                  {%if product.metafields['product-details']['primary-details'] %}
                   <div class="product__details">
                    <h2 class="product__details__headline">
                      Details
                    </h2>
                      {{ product.metafields['product-details']['primary-details'] }}
                   </div>    
                  {%else%}
                    <div class="product-secondary-info__description">
                      {{ product.metafields['secondary-info']['Secondary-info__Body'] }}
                    </div>
                  {%endif%}
                </div>
            </div>
          </div>
      </div>
  </section>
  
  {%if  product.metafields['secondary-info']['Secondary-info__Headline'] %}
  <section class="product-secondary-info">
    <div class="row">
      <div class="col-sm-12 col-lg-6"> 
          <div class="product-secondary-info__body-copy">
            <h1 class="headline  product-secondary-info__headline">
              {{ product.metafields['secondary-info']['Secondary-info__Headline'] }}
            </h1>
            <div class="product-secondary-info__description">
              {{ product.metafields['secondary-info']['Secondary-info__Body'] }}
            </div>
          </div>
      </div>

      <div class="col-sm-12 col-lg-6">
        <ul class="product-secondary-info__list">
          {%for  detail in product.metafields['secondary-details']  %}
            <li>
              <div class="product-secondary-info__list__item">
                <div class="product-secondary-info__list__item__icon">

{% schema %}
  {
    "name": "Featured Quote Section",
    "settings": [
      {
        "type": "text",
        "id": "headline",
        "label": "Headline",
        "default": "This is  headline."
      },
       {
        "type": "text",
        "id": "cta-label",
        "label": "Call to Action Label"
      },
       {
        "type": "url",
        "id": "cta-link",
        "label": "Call to Action Link"
      }
    ],
    "presets": [
      {
        "name": "Featured Quote Section",
        "category": "Copy Sections"
      }
    ]
  }

{% endschema %}
